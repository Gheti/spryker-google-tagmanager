<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="GooleTagManagerDefault">
        <property name="pageType" type="string" />
        <property name="store" type="string" />
        <property name="internalTraffic" type="bool" />
        <property name="currency" type="string" />
        <property name="externalIdHash" type="string" />
    </transfer>

    <transfer name="GooleTagManagerQuote">
        <property name="transactionEntity" type="string" />
        <property name="transactionId" type="string" />
        <property name="transactionAffiliation" type="string" />
        <property name="transactionTotal" type="string" />
        <property name="transactionTotalWithoutShippingAmount" type="string" />
        <property name="transactionSubtotal" type="string" />
        <property name="transactionTax" type="string" />
        <property name="transactionShipping" type="string" />
        <property name="transactionPayment" type="string" />
        <property name="transactionCurrency" type="string" />
        <property name="transactionProducts" type="GooleTagManagerTransactionProduct[]" />
        <property name="transactionProductsSkus" type="string[]" />
        <property name="customerEmail" type="string" />
    </transfer>

    <transfer name="GooleTagManagerTransaction">
        <property name="transactionEntity" type="string" />
        <property name="transactionId" type="string" />
        <property name="transactionAffiliation" type="string" />
        <property name="transactionTotal" type="string" />
        <property name="transactionTotalWithoutShippingAmount" type="string" />
        <property name="transactionSubtotal" type="string" />
        <property name="transactionTax" type="string" />
        <property name="transactionShipping" type="string" />
        <property name="transactionPayment" type="string" />
        <property name="transactionCurrency" type="string" />
        <property name="transactionProducts" type="GooleTagManagerTransactionProduct[]" />
        <property name="transactionProductsSkus" type="string[]" />
        <property name="transactionDate" type="string" />
        <property name="customerEmail" type="string" />
    </transfer>

    <transfer name="GooleTagManagerProductDetail">
        <property name="productId" type="int" />
        <property name="productName" type="string" />
        <property name="productSku" type="string" />
        <property name="productPrice" type="float" />
        <property name="productPriceExcludingTax" type="float" />
        <property name="productTax" type="float" />
        <property name="productTaxRate" type="float" />
        <property name="salePrice" type="float" />
    </transfer>

    <transfer name="GooleTagManagerTransactionProduct">
        <property name="id" type="int" />
        <property name="name" type="string" />
        <property name="sku" type="string" />
        <property name="price" type="float" />
        <property name="priceExcludingTax" type="float" />
        <property name="tax" type="float" />
        <property name="taxRate" type="float" />
        <property name="quantity" type="int" />
        <property name="brand" type="string" />
        <property name="ean" type="string" />
        <property name="url" type="string" />
        <property name="imageUrl" type="string" />
    </transfer>

    <transfer name="GoogleTagManagerNewsletterData">
        <property name="email" type="string" />
        <property name="externalIdHash" type="string" />
    </transfer>

    <transfer name="GooleTagManagerCategory">
        <property name="idCategory" type="int" />
        <property name="contentType" type="string" />
        <property name="name" type="string" />
        <property name="categorySize" type="int" />
        <property name="categoryProducts" type="GooleTagManagerCategoryProduct[]" />
        <property name="products" type="array"/>
    </transfer>

    <transfer name="GooleTagManagerCategoryProduct">
        <property name="idProductAbstract" type="int" />
        <property name="name" type="string" />
        <property name="sku" type="string" />
        <property name="price" type="float" />
    </transfer>


    <transfer name="EnhancedEcommerce">
        <property name="event" type="string" />
        <property name="eventCategory" type="string" />
        <property name="eventAction" type="string" />
        <property name="eventLabel" type="string" />
        <property name="ecommerce" type="array" />
        <property name="ecCheckoutOption" type="array" />
    </transfer>

    <transfer name="EnhancedEcommerceProduct">
        <property name="id" type="string" />
        <property name="name" type="string" />
        <property name="variant" type="string" />
        <property name="brand" type="string" />
        <property name="dimension10" type="string" />
        <property name="quantity" type="string" />
        <property name="price" type="string" />
        <property name="coupon" type="string" />
    </transfer>

    <transfer name="EnhancedEcommerceProductImpression">
        <property name="id" type="string" />
        <property name="name" type="string" />
        <property name="variant" type="string" />
        <property name="price" type="string" />
        <property name="list" type="string" />
        <property name="position" type="int" />
    </transfer>

    <transfer name="EnhancedEcommerceProductData">
        <property name="productAbstractId" type="int" />
        <property name="sku" type="string" />
        <property name="quantity" type="int" />
        <property name="price" type="string" />
    </transfer>

    <transfer name="ProductAbstract">
        <property name="name" type="string" />
    </transfer>

    <transfer name="Item">
        <property name="idProductAbstract" type="int"/>
        <property name="quantity" type="int"/>
        <property name="unitPrice" type="int"/>
        <property name="sku" type="string"/>
    </transfer>

    <transfer name="Quote">
        <property name="items" type="Item[]" singular="item"/>
    </transfer>

    <transfer name="Order">
        <property name="totals" type="Totals"/>
        <property name="calculatedDiscounts" type="CalculatedDiscount[]" singular="calculatedDiscount"/>
    </transfer>

    <transfer name="Totals">
        <property name="discountTotal" type="int"/>
    </transfer>

    <transfer name="CalculatedDiscount">
        <property name="voucherCode" type="string"/>
    </transfer>

</transfers>
